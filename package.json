{
  "name": "bindfiles",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "<20.0.0"
  },
  "scripts": {
    "dev": "vite dev",
    "start": "ts-node -T --esm --experimental-specifier-resolution=node ./src/server",
    "build": "vite build",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@hocuspocus/extension-sqlite": "^2.4.0",
    "@hocuspocus/provider": "^2.4.0",
    "@hocuspocus/transformer": "^2.4.0",
    "@tiptap/core": "^2.1.6",
    "@tiptap/extension-collaboration": "^2.1.6",
    "@tiptap/pm": "^2.1.6",
    "svelte-fa": "^3.0.4",
    "svelte-tree-view": "^1.4.2",
    "y-indexeddb": "^9.0.11",
    "y-prosemirror": "^1.2.1",
    "y-protocols": "^1.0.5",
    "y-websocket": "^1.5.0",
    "yjs": "^13.6.7"
  },
  "devDependencies": {
    "@hocuspocus/server": "^2.4.0",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.20.4",
    "@types/express": "^4.17.17",
    "@types/express-ws": "^3.0.1",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.33.0",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.66.1",
    "svelte": "^4.2.0",
    "svelte-check": "^3.4.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vitest": "^0.32.2",
    "ws": "^8.13.0"
  },
  "type": "module",
  "packageManager": "yarn@3.6.3"
}
